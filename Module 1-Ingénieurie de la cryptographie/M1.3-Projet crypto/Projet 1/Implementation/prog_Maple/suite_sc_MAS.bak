local n,z,i,B,A,e;
n := nops(r);
z := 0:
for i from 1 to n do
z := z+r[i];
end do;
B := (rand(z .. 2*z))();
A:=B;
while gcd(A,B)&gt;1 do
A:=rand(2..B-1)();
end do;
e:=[seq(A*r[i] mod B,i=1..n)];
return(B,A,r, e);
end proc:
